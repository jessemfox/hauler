
<% if (!(user.id===cUser)){ %>
	<button id='follow-button' class='btn btn-primary'>
		<%= (following ? "Following" : 'Follow') %>
		

	</button>
<% } %>

<button id='followed-users' class='btn btn-default'><%= followed_users.models.length %> Following </button>
<button id='followers' class='btn btn-default'><%= followers.models.length %> Followers </button>


<ul>
<% for (var i = 0; i<hauls.models.length; i++){ %>
	<li>
		<a href='/#/hauls/<%= hauls.models[i].id%>'> <%= hauls.models[i].get('title') %></a>
	</li>

<% } %>
</ul>

<div class='row cover'></div>